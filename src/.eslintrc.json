{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["google", "prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "require-jsdoc": 0,
    "require-await": "error",
    "new-cap": "off"
  }
}
